<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2D3447;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.javaprojectpractise1.CourseManagerFX">
    <children>
        <ComboBox fx:id="semesterComboBox"  layoutX="14.0" layoutY="27.0" prefWidth="150.0" promptText="Choose Semester" onAction="#semesterComboBoxAction" />
        <Label layoutX="174.0" layoutY="27.0" text="Courses:" textFill="WHITE">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <ListView fx:id="courseListView"  layoutX="14.0" layoutY="64.0" prefHeight="200.0" prefWidth="273.0" />
        <Label fx:id="label" layoutX="312.0" layoutY="27.0" text="PRE-REQUISITES:" textFill="WHITE">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <ListView fx:id="prerequisitesListView" layoutX="312.0" layoutY="64.0" prefHeight="200.0" prefWidth="273.0" />
        <Button fx:id="BackButton" onAction="#BackButtonAction" layoutX="14.0" layoutY="364.0" mnemonicParsing="false" text="Back" />
    </children>
</AnchorPane>
